{
    "name": "raju-cimpress/php-server-jwt-auth",
    "description": "PHP library for supporting Cimpress and Auth0 JWT on server side",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "firebase/php-jwt": "^6.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CimpressJwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Raju Mourya",
            "email": "raju.mourya@pixartprinting.com"
        }
    ]
}
