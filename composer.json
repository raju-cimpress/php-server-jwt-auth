{
    "name": "raju-cimpress/php-server-jwt-auth",
    "description": "PHP library for supporting Cimpress and Auth0 JWT on server side",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "psr/cache": "^2.0.0",
        "guzzlehttp/guzzle": "^7.7.0",
        "firebase/php-jwt": "^6.8"
    },
    "require-dev": {
        "symfony/cache": "^6.4",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CimpressJwtAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CimpressJwtAuth\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Raju Mourya",
            "email": "raju.mourya@pixartprinting.com"
        }
    ]
}
